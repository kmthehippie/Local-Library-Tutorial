<!DOCTYPE html>
<html lang="en">
<%- include('./partials/header') %>
<body>
  <div class="container-fluid">
    <div class="row">
        <div class="col-sm-2">
            <%- include('partials/sidebar') %>
        </div>
        <div class="col-sm-10">
          <h1><%= title %></h1>
            <% if(book_list.length>0){%>
                <ul>
                    <% book_list.forEach(book=>{ %>
                        <li><a href="<%=book.url%>"><%= book.title %></a> (<%=book.author.name%>)</li>
                    <% }) %>
                </ul>
            <% } else {%>
                <p> There are no Books.</p>
            <% } %>

        </div>
    </div>
</div>
</body>
</html>