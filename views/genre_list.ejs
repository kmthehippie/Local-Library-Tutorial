<!DOCTYPE html>
<html lang="en">
<%- include('./partials/header') %>
<body>
  <div class="container-fluid">
    <div class="row">
        <div class="col-sm-2">
            <%- include('partials/sidebar') %>
        </div>
        <div class="col-sm-10">
          <h1><%= title %></h1>
            <% if(genre_list.length>0){%>
                <ul>
                    <% genre_list.forEach(genre=>{ %>
                        <li>
                            <a href="<%=genre.url%>"><%= genre.name %></a> 
                        </li>
                    <% }) %>
                </ul>
            <% } else {%>
                <p> There are no genres.</p>
            <% } %>

        </div>
    </div>
</div>
</body>
</html>